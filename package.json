    "build:dev:body": "yarn build:dev --env.chunks=body",
    "build:dev:vendor": "yarn build:dev --env.chunks=vendor",
    "build:dev:vendor:analyze": "yarn build:dev --env.chunks=vendor --env.addons=bundleanalyze",
    "build:dist:vendor": "yarn build:dist --env.chunks=vendor",
    "build:dist:vendor:analyze": "yarn build:dist --env.chunks=vendor --env.addons=bundleanalyze",
    "build:dist": "yarn build --env.env=dist",
    "build:dev": "yarn build --env.env=dev",
