{
  "name": "dcf",
  "version": "1.0.0",
  "description": "Digital Campus Framework",
  "repository": {
    "type": "git",
    "url": "git://github.com/d-c-n/dcf.git"
  },
  "author": {
    "name": "Digital Campus Nebraska",
    "url": "http://digitalcampus.us/"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/d-c-n/dcf/issues"
  },
  "homepage": "https://github.com/d-c-n/dcf#readme",
  "devDependencies": {
    "ajv": "^6.4.0",
    "ansi-colors": "^1.1.0",
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "csso": "^3.5.0",
    "fancy-log": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-uglify-es": "^1.0.1",
    "modularscale-sass": "^3.0.5",
    "moment": "^2.22.1",
    "node-sass": "^4.9.0",
    "postcss-normalize": "^4.0.0",
    "stylelint": "^9.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.0",
    "sync-exec": "^0.6.2",
    "tar-fs": "^1.16.2",
    "uglify-save-license": "^0.4.1"
  },
  "dependencies": {
    "dialog-polyfill": "^0.4.9",
    "object-fit-images": "^3.2.3",
    "picturefill": "^3.0.2"
  },
  "engines": {
    "node": "node >= 0.10.0"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Explorer version",
    "last 1 Firefox version",
    "Firefox ESR",
    "last 1 Safari version",
    "Android >= 4.0",
    "BlackBerry >= 4.0",
    "iOS >= 10",
    "> 1% in my stats"
  ],
  "paths": {
    "src": {
      "base": "assets/src/",
      "scss": "assets/src/scss/",
      "vendorCss": "assets/src/vendorCss/",
      "fonts": "assets/src/fonts/",
      "json": "assets/src/json/",
      "js": "assets/src/js/",
      "appJs": "assets/src/js/app",
      "mustardJs": "assets/src/js/mustard",
      "vendorJs": "assets/src/js/vendor",
      "img": "assets/src/img/",
      "svg": "assets/src/svg/"
    },
    "build": {
      "base": "assets/build/",
      "css": "assets/build/css/",
      "fonts": "assets/build/fonts/",
      "js": "assets/build/js/",
      "appJs": "assets/build/js/app",
      "mustardJs": "assets/build/js/mustard",
      "vendorJs": "assets/build/js/vendor",
      "html": "assets/build/html/",
      "img": "assets/build/img/"
    },
    "dist": {
      "base": "assets/dist/",
      "css": "assets/dist/css/",
      "js": "assets/dist/js/",
      "fonts": "assets/dist/fonts/",
      "img": "assets/dist/img/",
      "svg": "assets/dist/svg/"
    }
  },
  "globs": {
    "buildJs": [
      "assets/src/js/**/*.js",
      "!assets/src/js/vendor/**/*.js",
      "!assets/src/js/mustard/**/*.js"
    ],
    "buildVendorCss": [
      "assets/src/vendorCss/**/*.css",
      "node_modules/dialog-polyfill/dialog-polyfill.css"
    ],
    "buildVendorJs": [
      "assets/src/js/vendor/**/*.js",
      "node_modules/dialog-polyfill/dialog-polyfill.js"
    ],
    "buildMustardJs": [
      "node_modules/picturefill/dist/picturefill.js",
      "node_modules/object-fit-images/dist/ofi.js",
      "assets/src/js/mustard/**/*.js"
    ],
    "distJSOptional": [
      "assets/build/js/app/optional/**/*.js"
    ],
    "distJSCommon": [
      "assets/build/js/app/common/**/*.js"
    ],
    "distVendorJs": [
      "assets/build/js/vendor/**/*.js"
    ],
    "distMustardJs": [
      "assets/build/js/mustard/**/*.js"
    ]
  },
  "outputNames": {
    "build": {
      "appCommon": "commonApp.concat.js",
      "vendor": "vendor.concat.js",
      "mustard": "mustard.concat.js"
    },
    "dist": {
      "appCommon": "dcf.commonApp.min.js",
      "vendor": "dcf.vendor.min.js",
      "mustard": "dcf.mustard.min.js"
    }
  }
}
